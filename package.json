{
  "name": "outputdirectedtheoremproving",
  "displayName": "OutputDirectedTheoremProving",
  "description": "Ai Agent that helps with output directed theorem proving in Coq. ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:outputdirectedtheoremproving.openProofState",
    "onCommand:outputdirectedtheoremproving.setOpenAiApiKey",
    "onCommand:outputdirectedtheoremproving.getDefaultChatModel",
    "onCommand:outputdirectedtheoremproving.changeLLMModel",
    "onLanguage:coq"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "outputdirectedtheoremproving.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "outputdirectedtheoremproving.openProofState",
        "title": "Open Coq Proof State"
      },
      {
        "command": "outputdirectedtheoremproving.setOpenAiApiKey",
        "title": "Set OpenAI API Key"
      },
      {
        "command": "outputdirectedtheoremproving.getDefaultChatModel",
        "title": "Select LLM Service"
      },
      {
        "command": "outputdirectedtheoremproving.changeLLMModel",
        "title": "Change LLM Model"
      }
    ],
    "chatParticipants": [
      {
        "id": "coq.llmChat",
        "fullName": "Coq Prover AI",
        "name": "coqproverai",
        "when": "resourceExtname == .v",
        "isSticky": true,
        "description": "What can I help you with regarding Coq?"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js && npm run compile-webview",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "watch:webview": "npm run watch-webview",
    "package": "npm run check-types && npm run lint && node esbuild.js --production && npm run compile-webview:production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "comment-new-scripts": "--- These are the new scripts for the webview ---",
    "compile-webview": "esbuild ./src/webview/main.js --bundle --outfile=src/webview/proofState.js --platform=browser --external:vscode",
    "watch-webview": "esbuild ./src/webview/main.js --bundle --outfile=src/webview/proofState.js --platform=browser --external:vscode --watch",
    "compile-webview:production": "esbuild ./src/webview/main.js --bundle --outfile=src/webview/proofState.js --platform=browser --external:vscode --minify"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.18.8",
    "@types/tmp": "^0.2.6",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.12",
    "eslint": "^9.34.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@handlewithcare/prosemirror-suggest-changes": "^0.1.7",
    "ajv": "^8.17.1",
    "async-mutex": "^0.5.0",
    "axios": "^1.12.2",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "openai": "^6.3.0",
    "prosemirror-example-setup": "^1.2.3",
    "prosemirror-model": "^1.25.4",
    "prosemirror-schema-basic": "^1.2.4",
    "prosemirror-state": "^1.4.4",
    "prosemirror-transform": "^1.10.4",
    "prosemirror-view": "^1.41.3",
    "tiktoken": "^1.0.22",
    "tmp": "^0.2.5",
    "ts-results": "^3.3.0",
    "vscode-languageclient": "^9.0.1"
  }
}
